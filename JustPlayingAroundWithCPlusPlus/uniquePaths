#include <iostream>
using namespace std;
int factorial(int num){
  int product  = 1;
  for(int i= num; i>0; i--){
    product  *= i;
  }
  return product;
}
int countAllPossiblePaths(int height, int width){
  int squares = (height-1) + (width - 1);
  int paths = factorial(squares)/(factorial(height-1)*factorial(squares-height+1));
  return paths;
}
int countAllPossiblePathsWithObstacles(int length, int width,int obstacleX, int obstacleY){
  return 0;
}
int main() {
  cout << countAllPossiblePaths(5, 5) << endl;

}
